  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "build_watch": "tsc -w --pretty",
    "nodemon_app_watch": "nodemon --watch src --watch dist ./dist/server.js",
    "dev:serial": "npm-run-all --serial build_watch nodemon_app_watch",
    "dev:parallel": "concurrently -k \"npm run build_watch\" \"npm run nodemon_app_watch\"",
    "dev:start": "npm-run-all --serial build_watch nodemon_app_watch start"
  },
